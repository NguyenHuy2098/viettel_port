  },
  "dependencies": {
    "@coreui/coreui": "^2.1.12",
    "@coreui/icons": "0.3.0",
    "@coreui/react": "^2.5.1",
    "@types/react-datepicker": "^2.3.0",
    "@types/react-paginate": "^6.2.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.5.0",
    "core-js": "^3.1.4",
    "env-cmd": "^9.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "flag-icon-css": "^3.3.0",
    "font-awesome": "^4.7.0",
    "highcharts": "^7.1.2",
    "highcharts-react-official": "^2.2.2",
    "i18next": "^17.0.6",
    "immer": "^3.1.3",
    "lodash": "^4.17.14",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "oidc-client": "^1.8.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-datepicker": "^2.8.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-i18next": "^10.11.4",
    "react-loadable": "^5.5.0",
    "react-paginate": "^6.3.0",
    "react-redux": "^7.1.0",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.8.6",
    "reactstrap": "^8.0.1",
    "redux": "^4.0.4",
    "redux-oidc": "^3.1.2",
    "redux-saga": "^1.0.5",
    "redux-starter-kit": "^0.5.1",
    "redux-unfold-saga": "^1.3.0",
    "simple-line-icons": "^2.4.1",
    "typescript": "^3.5.3",
    "xlsx-populate": "^1.19.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/gitlab": "^3.1.7",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.2",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.2",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-loadable": "^5.5.1",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/reactstrap": "^8.0.1",
    "@types/yup": "^0.26.21",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.0",
    "i18next-scanner": "^2.10.2",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "react-scripts": "^3.0.1",
    "semantic-release": "^15.13.18",
    "standard-changelog": "^2.0.11"
  },
  "scripts": {
    "build:development": "env-cmd -e development react-scripts build",
    "build:production": "env-cmd -e production react-scripts build",
    "changelog": "standard-changelog && git add CHANGELOG.md",
    "commit": "npx git-cz",
    "eject": "react-scripts eject",
    "lint": "npm run lint:eslint:fix -- .",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern public/ --ext .js,.jsx,.tsx,.ts",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "semantic-release": "semantic-release",
    "serve": "npx serve -s build --no-clipboard",
    "start": "env-cmd -e development react-scripts start",
    "test": "react-scripts test",
    "test:cov": "npm test -- --coverage --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --runInBand"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --config .eslintrc.js --ignore-path .gitignore --ignore-pattern static/ --ext .js,.ts,.tsx --fix",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*index.js",
      "!src/serviceWorker.js",
      "!src/polyfill.js"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "README.md",
            "yarn.lock"
          ]
        }
      ]
    ]
  }
}
