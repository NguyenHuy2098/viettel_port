    "i18next-scanner": "^2.10.3",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "react-scripts": "^3.2.0",
    "semantic-release": "^15.13.30",
    "standard-changelog": "^2.0.17"
  },
  "scripts": {
    "build": "react-scripts build",
    "build:development": "env-cmd -e development react-scripts build",
    "build:qas": "env-cmd -e qas react-scripts build",
    "build:production": "env-cmd -e production react-scripts build",
    "changelog": "standard-changelog && git add CHANGELOG.md",
    "commit": "npx git-cz",
    "eject": "react-scripts eject",
    "lint": "npm run lint:eslint:fix -- .",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern public/ --ext .js,.jsx,.tsx,.ts",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "semantic-release": "semantic-release",
    "serve": "npx serve -s build --no-clipboard",
    "start": "env-cmd -e development react-scripts start",
    "test": "react-scripts test",
    "test:cov": "npm test -- --coverage --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "generate-type-api": "npx dtsgenerator --out src/@types/types.d.ts --url https://se.publicapi-in.viettelpost.vn/swagger/v1/swagger.json",
    "tsc": "tsc -p ./ --noEmit --skipLibCheck",
    "tsc:watch": "npm run tsc -- --watch"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --config .eslintrc.js --ignore-path .gitignore --ignore-pattern static/ --ext .js,.ts,.tsx --fix",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*index.js",
      "!src/serviceWorker.js",
      "!src/polyfill.js"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "README.md",
            "yarn.lock"
          ]
        }
      ]
    ]
  }
}
